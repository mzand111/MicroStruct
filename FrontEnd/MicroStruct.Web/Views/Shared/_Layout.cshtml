        //userOrganizationUnitTitle = claimsIdentity.FindFirst("OrganizationUnitTitle");
        //userImpersonation = claimsIdentity.FindFirst("UserImpersonation");
        //originalUsername = claimsIdentity.FindFirst("OriginalUsername");
    }
}
else
{


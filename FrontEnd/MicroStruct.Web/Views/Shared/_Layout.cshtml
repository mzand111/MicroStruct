@{
    Layout = null;
    System.Security.Claims.Claim userFirstName = null;
    System.Security.Claims.Claim userLastName = null;
    // System.Security.Claims.Claim userRole = null;
    List<string> userRoleName = null;
    //System.Security.Claims.Claim userUniversityID = null;
    //System.Security.Claims.Claim userOrganizationUnitID = null;
    //System.Security.Claims.Claim userOrganizationOfficeID = null;
    //System.Security.Claims.Claim userUniversityName = null;
    //System.Security.Claims.Claim userOrganizationUnitTitle = null;
    //System.Security.Claims.Claim userOrganizationOfficeTitle = null;
    //System.Security.Claims.Claim userHasVisitingResponsibility = null;
    //System.Security.Claims.Claim userImpersonation = null;
    //System.Security.Claims.Claim originalUsername = null;
    var UserRoleNameShowToUser = "";
}


@if (User.Identity.IsAuthenticated)
{

    var claimsIdentity = User.Identity as System.Security.Claims.ClaimsIdentity;

    if (claimsIdentity != null)
    {
        userFirstName = claimsIdentity.FindFirst("Given_Name");
        userLastName = claimsIdentity.FindFirst("family_name");
        userRoleName = claimsIdentity.FindAll("role").Select(a => a.Value).ToList();

        //userUniversityID = claimsIdentity.FindFirst("UniversityID");
        //userOrganizationUnitID = claimsIdentity.FindFirst("OrganizationUnitID");
        //userOrganizationOfficeID = claimsIdentity.FindFirst("OrganizationOfficeID");
